{
    "name": "simialbi/yii2-form-builder",
    "type": "yii2-extension",
    "description": "Form Builder module for yii2",
    "keywords": [
        "yii2",
        "forms",
        "form",
        "form builder",
        "code generator"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "simialbi",
            "email": "simi.albi@outlook.com"
        }
    ],
    "support": {
        "source": "https://github.com/simialbi/yii2-form-builder",
        "issues": "https://github.com/simialbi/yii2-form-builder/issues"
    },
    "minimum-stability": "beta",
    "require": {
        "php": ">=7.0",
        "yiisoft/yii2": "^2.0.20",
        "yiisoft/yii2-bootstrap4": "^2.0.4",
        "arogachev/yii2-sortable": "^0.1.6",
        "fortawesome/font-awesome": "^5.12.0",
        "kartik-v/yii2-grid": "^3.3.0",
        "kartik-v/yii2-widget-datepicker": "^1.4.0",
        "kartik-v/yii2-widget-select2": "^2.1.0",
        "npm-asset/highlight.js": "^9.18.1",
        "npm-asset/jsoneditor": "^9.1.4",
        "npm-asset/json-editor--json-editor": "^2.5.4",
        "simialbi/yii2-fontawesome": "^3.4.8",
        "simialbi/yii2-simialbi-base": ">=0.10.2 <1.0 | ^1.0.0",
        "simialbi/yii2-summernote": "^1.1.0",
        "simialbi/yii2-widget-datedropper": "^1.0.0-beta.2",
        "simialbi/yii2-widget-timedropper": "^1.0.0-beta.4"
    },
    "require-dev": {
        "yiisoft/yii2-coding-standards": "~2.0",
        "phpunit/phpunit": "<7"
    },
    "autoload": {
        "psr-4": {
            "simialbi\\yii2\\formbuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yiiunit\\extensions\\formbuilder\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
